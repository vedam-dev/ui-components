		"webpack": "^5.96.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"jestSonar": {
		"reportPath": "build/reports"
	},
	"lint-staged": {
		"{,src/**/}*.{md,json,ts,tsx,css,scss,yml}": [
			"yarn lint:fix",
			"git add"
		]
	}
